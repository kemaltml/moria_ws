    <!--::::::::::IMU SENSOR::::::::::-->

    <joint name="imu_joint" type="fixed">
      <parent>base</parent>
      <child>imu_link</child>
      <pose>0 0 -0.33 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
