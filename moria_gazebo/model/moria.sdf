    <link name="camera_rgb_frame">
      <inertial>
        <pose>0.005 0.018 0.013 0 -1.57 -1.57</pose>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
