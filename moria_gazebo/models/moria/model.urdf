  <!-- ::::::::::LIDAR::::::::::-->
  <joint name="lidar_joint" type="fixed">
    <parent link="leg"/>
    <child link="lidar"/>
    <origin rpy="0 0 0" xyz="0.1335 0.0005 0.0156"/>
  </joint>

  <link name="lidar">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="model://moria/meshes/lidar.STL"/>
      </geometry>
      <material name="cyan">
        <color rgba="0.3137 0.6941 0.7843 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="model://moria/meshes/lidar.STL"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.33"/>
      <inertia ixx="0.00061" ixy="0.0" ixz="-0.00015" iyy="0.00097" iyz="0.0" izz="0.00110"/>
    </inertial>
  </link>
  <gazebo reference="lidar">
    <material>Gazebo/Blue</material>
    <sensor name="lidar" type="ray">
      <pose>0.1335 0.0005 0.0156 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>180</samples>
            <min_angle>-1.57</min_angle>
            <max_angle>1.57</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.2</min>
          <max>12</max>
        </range>
      </ray>
      <plugin filename="libgazebo_ros_ray_sensor.so" name="laser_controller">
        <ros>
          <remapping>~/out:=scan</remapping>
        </ros>
        <output_type>sensor_msgs/LaserScan</output_type>
        <frame_name>lidar</frame_name>
      </plugin>
    </sensor>
  </gazebo>




