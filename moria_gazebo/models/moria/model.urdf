      <camera>
        <horizontal_fov>1.5</horizontal_fov>
        <image>
          <format>R8G8B8</format>
          <width>640</width>
          <height>480</height>
        </image>
        <clip>
          <near>0.3</near>
          <far>3.0</far>
        </clip>
      </camera>
      <plugin filename="libgazebo_ros_camera.so" name="camera_controller_depth">
        <frame_name>camera_depth_frame</frame_name>
        <min_depth>0.3</min_depth>
        <max_depth>3.0</max_depth>
      </plugin>
    </sensor>
  </gazebo>




  <!-- ::::::::::::::::::::DRIVER DEFINITIONS FOR GAZEBO::::::::::::::::::::-->
  <!-- ::::::::::DIFF DRIVE::::::::::-->
  <gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="moria_diff_drive">
      <update_rate>30</update_rate>

      <left_joint>left_wheel_joint</left_joint>
      <right_joint>right_wheel_joint</right_joint>

      <wheel_separation>0.344</wheel_separation>
      <wheel_diameter>0.06</wheel_diameter>

      <max_wheel_torque>8.4</max_wheel_torque>

      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>

      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <publish_wheel_tf>true</publish_wheel_tf>
    </plugin>
  </gazebo>

  <!-- ::::::::::JOINT STATE::::::::::-->
  <gazebo>
    <plugin filename="libgazebo_ros_joint_state_publisher.so" name="moria_joint_states">
    <ros>
        <remapping>~/out:=joint_states</remapping>
      </ros>
      <update_rate>30</update_rate>
      <joint_name>front_radial_joint</joint_name>
      <joint_name>front_wheel_joint</joint_name>
      <joint_name>right_wheel_joint</joint_name>
      <joint_name>left_wheel_joint</joint_name>
    </plugin>
  </gazebo>

<!--::::::::::ROS2 CONTROL::::::::::-->
  <ros2_control name="GazeboSystem" type="system">
    <hardware>
      <plugin>gazebo_ros2_control/GazeboSystem</plugin>
    </hardware>
    <joint name="left_wheel_joint">
      <command_interface name="velocity">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <state_interface name="velocity"/>
      <state_interface name="position"/>
    </joint>

    <joint name="right_wheel_joint">
      <command_interface name="velocity">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <state_interface name="velocity"/>
      <state_interface name="position"/>
    </joint>

    <joint name="front_radial_joint">
      <command_interface name="velocity">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <state_interface name="velocity"/>
      <state_interface name="position"/>
    </joint>

    <joint name="front_wheel_joint">
      <command_interface name="velocity">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <state_interface name="velocity"/>
      <state_interface name="position"/>
    </joint>
  </ros2_control>
</robot>
