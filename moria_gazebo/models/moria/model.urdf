  <joint name="camera_rgb_joint" type="fixed">
    <parent link="camera"/>
    <child link="camera_rgb_frame"/>
    <origin rpy="0 0 0" xyz="0 0.02 0.015"/>
  </joint>
  <link name="camera_rgb_frame"/>

  <joint name="camera_point_joint" type="fixed">
    <parent link="camera"/>
    <child link="camera_point_frame"/>
    <origin rpy="1.57 3.14 0" xyz="0 0.02 0.015"/>
  </joint>
  <link name="camera_point_frame"/>
  
  <gazebo reference="camera_rgb_frame">
    <sensor name="camera_rgb" type="camera">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>30</update_rate>
      <camera>
        <horizontal_fov>1.5</horizontal_fov>
        <image>
          <format>R8G8B8</format>
          <width>640</width>
          <height>480</height>
        </image>
        <clip>
          <near>0.3</near>
          <far>12.0</far>
        </clip>
      </camera>
      <plugin filename="libgazebo_ros_camera.so" name="camera_controller_rgb">
        <frame_name>camera_rgb_frame</frame_name>
      </plugin>
    </sensor>
  </gazebo>

