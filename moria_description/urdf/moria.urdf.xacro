<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="moria">

    

    <link name="base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/base_link.STL"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0667 0.2431 0.4078 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/base_link.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="4.44"/>
            <inertia 
                ixx="0.02125" ixy="0.0" ixz="0.00001"
                iyy="0.04177" iyz="0.0"
                izz="0.04948" />
        </inertial>
    </link>




    <!-- FRONT WHEEL -->
    <joint name="front_base_joint" type="fixed">
        <origin xyz="0.23 0 0" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="front_base"/>
    </joint>

    <link name="front_base">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/front_base.STL"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0667 0.2431 0.4078 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/front_base.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.003"/>
            <inertia 
                ixx="0.0001" ixy="0.0" ixz="0.0"
                iyy="0.0002" iyz="0.0"
                izz="0.0001" />
        </inertial>
    </link>


    <joint name="front_radial_joint" type="continuous">
        <parent link="front_base"/>
        <child link="front_radial"/>
        <origin xyz="0 0 0" rpy="0 0 -1.5708"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="front_radial">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/front_rad.STL"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0667 0.2431 0.4078 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/front_rad.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.01"/>
            <inertia 
                ixx="2.54E-06" ixy="0.0" ixz="0.0"
                iyy="2.17E-06" iyz="0.0"
                izz="3.02E-06" />
        </inertial>
    </link>


    <joint name="front_wheel_joint" type="continuous">
        <parent link="front_radial"/>
        <child link="front_wheel"/>
        <origin xyz="0 -0.022 -0.035" rpy="1.5708 0 1.5708"/>
        <axis xyz="0 0 -1"/>
    </joint>

    <link name="front_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/front_wheel.STL"/>
            </geometry>
            <material name="cyan">
                <color rgba="0.3137 0.6941 0.7843 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/front_wheel.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.006"/>
            <inertia 
                ixx="4.60E-07" ixy="0.0" ixz="0.0"
                iyy="4.60E-07" iyz="0.0"
                izz="8.66E-07" />
        </inertial>
    </link>

    


    <!-- RIGHT WHEEL-->
    <joint name="right_motor_joint" type="fixed">
        <parent link="base_link"/>
        <child link="right_motor"/>
        <origin xyz="0 -0.096 -0.02" rpy="0 0 0"/>
    </joint>

    <link name="right_motor">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/right_motor.STL"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0667 0.2431 0.4078 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/right_motor.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.11"/>
            <inertia 
                ixx="0.00003" ixy="0.0" ixz="0.0"
                iyy="0.00005" iyz="0.0"
                izz="0.00006" />
        </inertial>
    </link>


    <joint name="right_wheel_joint" type="continuous">
        <parent link="right_motor"/>
        <child link="right_wheel"/>
        <origin xyz="0 -0.076 0" rpy="-1.5708 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/right_wheel.STL"/>
            </geometry>
            <material name="cyan">
                <color rgba="0.3137 0.6941 0.7843 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/right_wheel.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.09111"/>
            <inertia 
                ixx="0.00003" ixy="0.0" ixz="0.0"
                iyy="0.00003" iyz="0.0"
                izz="0.00004" />
        </inertial>
    </link>




    <!-- LEFT WHEEL-->
    <joint name="left_motor_joint" type="fixed">
        <parent link="base_link"/>
        <child link="left_motor"/>
        <origin xyz="0 0.096 -0.02" rpy="0 0 0"/>
    </joint>

    <link name="left_motor">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/left_motor.STL"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0667 0.2431 0.4078 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/left_motor.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.11"/>
            <inertia 
                ixx="0.00003" ixy="0.0" ixz="0.0"
                iyy="0.00005" iyz="0.0"
                izz="0.00006" />
        </inertial>
    </link>


    <joint name="left_wheel_joint" type="continuous">
        <parent link="left_motor"/>
        <child link="left_wheel"/>
        <origin xyz="0 0.076 0" rpy="1.5708 0 0"/>
        <axis xyz="0 0 -1"/>
    </joint>

    <link name="left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/left_wheel.STL"/>
            </geometry>
            <material name="cyan">
                <color rgba="0.3137 0.6941 0.7843 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/left_wheel.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.09111"/>
            <inertia 
                ixx="0.00003" ixy="0.0" ixz="0.0"
                iyy="0.00003" iyz="0.0"
                izz="0.00004" />
        </inertial>
    </link>




    <!-- LEG -->
    <joint name="leg_joint" type="fixed">
        <parent link="base_link"/>
        <child link="leg"/>
        <origin xyz="0.076 0 0.185" rpy="0 0 0"/>
    </joint>

    <link name="leg">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/leg.STL"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0667 0.2431 0.4078 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/leg.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="2.366"/>
            <inertia 
                ixx="0.07510" ixy="0.00008" ixz="-0.00377"
                iyy="0.07190" iyz="0.00005"
                izz="0.02825" />
        </inertial>
    </link>




    <!-- BODY-->
    <joint name="body_link" type="fixed">
        <parent link="leg"/>
        <child link="body"/>
        <origin xyz="0 0 0.465" rpy="0 0 0"/>
    </joint>

    <link name="body">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/body.STL"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0667 0.2431 0.4078 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/body.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="2.80832"/>
            <inertia 
                ixx="0.05159" ixy="0.0" ixz="-0.00006"
                iyy="0.03871" iyz="-0.00004"
                izz="0.02383" />
        </inertial>
    </link>




    <!-- HEAD-->
    <joint name="neck_joint" type="revolute">
        <parent link="body"/>
        <child link="neck"/>
        <origin xyz="0.0092 0.0004 0.5471" rpy="0 0 1.5708"/>
        <axis xyz="0 0 1"/>
        <limit
            lower="-0.5"
            upper="0.5"
            effort="0"
            velocity="0"/>
    </joint>

    <link name="neck">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/neck.STL"/>
            </geometry>
            <material name="cyan">
                <color rgba="0.3137 0.6941 0.7843 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/neck.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.046"/>
            <inertia 
                ixx="0.00001" ixy="0.0" ixz="0.0"
                iyy="0.00001" iyz="0.0"
                izz="0.00001" />
        </inertial>
    </link>


    <joint name="head_joint" type="revolute">
        <parent link="neck"/>
        <child link="head"/>
        <origin xyz="0.0224 0.004 0.0403" rpy="1.5708 0 1.5708"/>
        <axis xyz="0 0 1"/>
        <limit
            lower="-0.2"
            upper="0.2"
            effort="0"
            velocity="0"/>        
    </joint>

    <link name="head">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/head.STL"/>
            </geometry>
            <material name="cyan">
                <color rgba="0.3137 0.6941 0.7843 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find moria_description)/meshes/head.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.7694"/>
            <inertia 
                ixx="0.0045" ixy="0.00005" ixz="0.0"
                iyy="0.0029" iyz="0.0"
                izz="0.0023" />
        </inertial>
    </link>

    <xacro:include filename="sensors.xacro"/>
    <xacro:include filename="right_arm.xacro"/>
    <xacro:include filename="left_arm.xacro"/>
    
</robot>
